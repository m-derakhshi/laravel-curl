{
  "name": "mderakhshi/laravel-curl",
  "description": "Custom PHP Curl library for the Laravel 6",
  "version": "dev-master",
  "keywords": [
    "masoud_derakhshi",
    "mderakhshi",
    "Laravel",
    "curl"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jan Oris",
      "email": "jan.oris@ixudra.be"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-curl": "*",
    "illuminate/support": ">=4.0"
  },
  "autoload": {
    "psr-4": {
      "mderakhshi\\Curl\\": "src/"
    },
    "files": [
      "src/CurlHelper.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "mderakhshi\\Curl\\CurlServiceProvider"
      ],
      "aliases": {
        "Curl": "mderakhshi\\Curl\\Facades\\Curl"
      }
    }
  }
}
